{
    "name": "th3mouk/cms-siteplan-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMSSiteplanBundle to generate site plan of SonataPageBundle and personals routes",
    "keywords": [
        "sonata", "cms", "site", "plan", "siteplan", "sitemap", "symfony", "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Marodon",
            "email": "marodon.jeremy@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "knplabs/knp-menu" : "^2.1",
        "sonata-project/page-bundle": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/event-dispatcher": "^2.3|^3.0",
        "symfony/http-kernel": "^2.3|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Th3Mouk\\CMSSiteplanBundle\\": "" }
